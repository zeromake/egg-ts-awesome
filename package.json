{
  "name": "egg-ts-awesome",
  "version": "1.0.0",
  "description": "egg-ts-awesome",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "format": "prettier --write \"{app,config,test,database}/**/*.ts\"",
    "start": "egg-scripts start --daemon --title=egg-ts-awesome",
    "stop": "egg-scripts stop --title=egg-ts-awesome",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -x database",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "sync": "npm run typeorm migration:run",
    "build:www": "cd ./views/www && npm run build",
    "build:admin": "cd ./views/admin && npm run build",
    "build": "npm run build:www && npm run build:admin"
  },
  "dependencies": {
    "@zeromake/egg-mock": "^3.24.2",
    "@zeromake/egg-swagger": "^0.1.3",
    "@zeromake/egg-typeorm": "^1.4.5",
    "egg": "^2.22.2",
    "egg-passport": "^2.1.0",
    "egg-redis": "^2.4.0",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.11.0",
    "egg-session-redis": "^2.1.0",
    "egg-view-ejs": "^2.0.0",
    "egg-view-nunjucks": "^2.2.0",
    "mysql2": "^1.6.5",
    "passport-local": "^1.0.0",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "openapi3-ts": "^1.3.0",
    "@types/fs-extra": "^8.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.2",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.8",
    "@types/swagger-schema-official": "^2.0.18",
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "codecov": "^3.7.1",
    "egg-bin": "^4.13.0",
    "egg-ci": "^1.12.0",
    "fs-extra": "^8.1.0",
    "prettier": "1.18.2",
    "sqlite3": "^4.0.9",
    "swagger-jsdoc": "^3.3.0",
    "swagger-schema-official": "^2.0.0-bab6bed",
    "swagger-ui-dist": "^3.23.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslib": "^1.10.0",
    "tslint": "^5.18.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "zeromake<a390720046@gmail.com>",
  "license": "MIT"
}
