{
  "name": "batch-sms",
  "version": "1.0.0",
  "description": "batch sms",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "format": "prettier --write \"{app,config,test,database}/**/*.ts\"",
    "start": "egg-scripts start --daemon --title=batch-sms",
    "stop": "egg-scripts stop --title=batch-sms",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@zeromake/egg-typeorm": "^1.4.1",
    "egg": "^2.22.2",
    "egg-passport": "^2.0.1",
    "egg-passport-local": "^1.2.1",
    "egg-redis": "^2.3.2",
    "egg-router-plus": "^1.3.0",
    "egg-scripts": "^2.11.0",
    "egg-session-redis": "^2.1.0",
    "mysql2": "^1.6.5",
    "passport-local": "^1.0.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.8",
    "@types/supertest": "^2.0.0",
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.13.1",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.22.2",
    "prettier": "1.17.0",
    "tslib": "^1.9.3",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "zeromake<a390720046@gmail.com>",
  "license": "MIT"
}
